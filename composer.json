{
    "name": "aliasproject/wp-routes",
    "type": "library",
    "description": "Package for WordPress Routing",
    "keywords": [
        "aliasproject",
        "wp-routes"
    ],
    "homepage": "https://github.com/aliasproject/wp-routes",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Aguiar",
            "email": "mike@aliasproject.com",
            "homepage": "https://michaelaguiar.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "AliasProject\\WPRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AliasProject\\WPRoutes\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
